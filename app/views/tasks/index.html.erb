<div class="container">
  <h1> My Tasks</h1>
  <% @tasks.each do |task| %>
  <ul>
    <li>
  <%= task.title %>
  <%= task.completed %>
  <%= link_to "detail", task_path(task) %>
    </li>
  </ul>
<%= link_to "edit", task_edit_path(task.id) %>
  <% end %>
</div>
<%= link_to "create", tasks_new_path %>
